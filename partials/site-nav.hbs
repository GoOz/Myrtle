<nav class="site-nav">
    <div class="site-nav-left">
        {{^is "home"}}
            {{#if @site.logo}}
                <a class="site-nav-logo" href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@site.url}}">{{@site.title}}</a>
            {{/if}}
        {{/is}}
        {{#if @site.navigation}}
            {{navigation}}
        {{/if}}
    </div>
    <div class="site-nav-right">
        <div class="social-links">
            <a class="social-link social-link-500" href="http://500px.com/CycoGoOz" target="_blank">{{> "icons/500px"}}</a>
            <a class="social-link social-link-ig" href="https://www.instagram.com/gooz/" target="_blank">{{> "icons/instagram"}}</a>
            <a class="social-link social-link-gh" href="https://github.com/GoOz" target="_blank">{{> "icons/github"}}</a>
            <a class="social-link social-link-mtd" href="https://mastodon.partipirate.org/@GoOz" target="_blank">{{> "icons/mastodon"}}</a>
            {{#if @site.twitter}}
                <a class="social-link social-link-tw" href="{{twitter_url @site.twitter}}" target="_blank">{{> "icons/twitter"}}</a>
            {{/if}}
            {{#if @site.facebook}}
            <a class="social-link social-link-fb" href="{{facebook_url @site.facebook}}" target="_blank">{{> "icons/facebook"}}</a>
            {{/if}}
        </div>
        {{#if @labs.subscribers}}
            <a class="subscribe-button" href="#subscribe">Subscribe</a>
        {{else}}
            <a class="rss-button" href="https://feedly.com/i/subscription/feed/{{@site.url}}/rss/" target="_blank">{{> "icons/rss"}}</a>
        {{/if}}
    </div>
</nav>
